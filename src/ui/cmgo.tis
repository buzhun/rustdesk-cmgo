function createCmgoConnect(id, type) {
    stderr.print(id);
    if (!id) return;
    var old_id = id;
    id = handler.handle_relay_id(id);
    var force_relay = old_id != id; 
    if (id == my_id) {
        msgbox("custom-error", "Error", "You cannot connect to your own computer");
        return;
    }
    handler.set_remote_id(id);
    handler.new_remote(id, type, force_relay);
}

class Cmgo: Reactor.Component {
    this var data = {};
    this var remote_id = "";

    function this(props,kids) {
        this.data = props.data ?? {};
        const workstationInfos = this.data.workstationInfos;
        const stationData = workstationInfos[0];
        const {deskClientUsername} = stationData;
        const remote_id = deskClientUsername.split(" ").join("")
        this.remote_id = remote_id;
        stdout.println(JSON.stringify(this.data, " "));
    }

    event click $(#connect) {
        this.newRemote("connect");
    }

    event click $(#file-transfer) {
        this.newRemote("file-transfer");
    }


    event click $(#anchor-rest) {
        $(#anchor-rest).popup(this.$(#popup-rest), (4 >> 16) | 6);
    } 

    event click $(#anchor-extra) {
        $(#anchor-extra).popup(this.$(#popup-extra), (4 >> 16) | 6);
    } 

    event click $(#anchor-user) {
        $(#anchor-user).popup(this.$(#popup-user), (4 >> 16) | 6);
    } 

    event click $(#anchor-pay) {
        $(#anchor-pay).popup(this.$(#popup-pay), (4 >> 16) | 6);
    } 

    event click $(.confirm) {
        $(#popup-pay).state.popup = false;
    }

    // function  test() {
    //     var tw = View.window {
    //         url: self.url("samples/popup/popup-trigger.htm")
    //     };
    // }

    function newRemote(type) {
        stdout.print(this.remote_id,type);
        createCmgoConnect(this.remote_id, type);
    }

    function render () {
        const data = this.data;
        const userInfo = data.userInfo;
        const workstationInfos = data.workstationInfos;
        const stationData = workstationInfos[0];
        const {name, userType, mobile, lastestOnLineTime} = userInfo;
        var { stationName, remainingTime, timeLengthAddedValue, totalTime} = stationData;

        return <div .cmgo>
           <div .left>
                <div .left-logo>{svg_cmgo}</div>
                <div .user-area .flex>
                    <div .avatar>{svg_avatar}</div>
                    <div .flex .nickname-area #anchor-user>
                        <div .nickname>{name}</div>
                        <div .arrow>
                            {svg_arrow}
                        </div>
                    </div>
                    <popup #popup-user>
                        <div .box>
                            <div .name>{name}</div>
                            <div .row>
                                <div .label>认证</div>
                                <div .company .value .flex>{svg_verify}{userType == 1 ? "个人设计师" : 
                                (userType == 2 || userType == 4) ? name : userType == 3 ? "学生" : ""}</div>
                            </div>
                            <div .row>
                                <div .label>手机</div>
                                <div .value>{mobile}</div>
                            </div>
                        </div>
                    </popup>
                </div>  
                <div .pay .flex #anchor-pay>{svg_pay}充值管理</div>  
                <popup #popup-pay>
                    <div .pay-box>
                        <div .title>提示</div>
                        <div .desc>请联系您的企业管理员进行充值</div>
                        <div .bottom>
                            <div .confirm .btn>
                                确定
                            </div>
                        </div>
                    </div>  
                </popup>
                <div .secure .flex>{svg_secure}安全加密链路</div>       
           </div>
           <div .cmgo-right> 
                <div .greet>您好，{name}</div>
                <div .station>
                    <div .type .flex>
                        <div .name>{stationName}</div>
                        {connect_status == 1 ? <div .status><span .status-icon></span>就绪</div>:
                        <div .status .warning><span .status-icon></span>未就绪，请检查网络连接</div>}
                    </div>
                    <div .last-time>上次使用 {lastestOnLineTime}</div>
                    <div .flex>
                        <div .detail>
                            <div .flex>
                                <div .label .flex>可用套餐 
                                    <div #anchor-rest>
                                        {svg_question}
                                    </div>
                                    <popup #popup-rest>
                                        <div .content>本套餐基础使用时间为8:00am-20:00pm</div>
                                    </popup>
                                </div>
                                <div .value>
                                    {remainingTime < 1 ? <span .warning>{remainingTime > 0 ? remainingTime : 0}</span>:
                                     remainingTime}
                                     <span .unit>天</span> / {totalTime}<span .unit>天</span>
                                     </div>
                            </div>
                            <div .flex .extra>
                                <div .label .flex>可用增选时长 
                                    <div #anchor-extra>
                                        {svg_question}
                                    </div>
                                    <popup #popup-extra>
                                        <div .content>8:00am-20:00pm外的使用时间</div>
                                    </popup>
                                </div>
                                <div .value>{timeLengthAddedValue > 2 ? timeLengthAddedValue : 
                                    <span .warning>{timeLengthAddedValue||0}</span>}
                                小时</div>
                            </div>
                        </div>
                        <div .opts .flex>
                            <div .btn .send-file-btn #file-transfer>传输文件</div>
                            <div .btn .connect-btn #connect>连接</div>
                        </div>
                    </div>
                </div>
           </div>
        </div>
    }
}